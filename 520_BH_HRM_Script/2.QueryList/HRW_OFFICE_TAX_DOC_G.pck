           , REPLACE(SUBSTR(LPAD(NVL(WD.A30_PAY_INCOME_TAX_AMT, 0) + NVL(WD.A30_PAY_SP_TAX_AMT, 0), 13, '*'), -8, 1), '*', '') AS SUM_NUM8
           , REPLACE(SUBSTR(LPAD(NVL(WD.A30_PAY_INCOME_TAX_AMT, 0) + NVL(WD.A30_PAY_SP_TAX_AMT, 0), 13, '*'), -9, 1), '*', '') AS SUM_NUM9
           , REPLACE(SUBSTR(LPAD(NVL(WD.A30_PAY_INCOME_TAX_AMT, 0) + NVL(WD.A30_PAY_SP_TAX_AMT, 0), 13, '*'), -10, 1), '*', '') AS SUM_NUM10
           , REPLACE(SUBSTR(LPAD(NVL(WD.A30_PAY_INCOME_TAX_AMT, 0) + NVL(WD.A30_PAY_SP_TAX_AMT, 0), 13, '*'), -11, 1), '*', '') AS SUM_NUM11
           , REPLACE(SUBSTR(LPAD(NVL(WD.A30_PAY_INCOME_TAX_AMT, 0) + NVL(WD.A30_PAY_SP_TAX_AMT, 0), 13, '*'), -12, 1), '*', '') AS SUM_NUM12
           , REPLACE(SUBSTR(LPAD(NVL(WD.A30_PAY_INCOME_TAX_AMT, 0) + NVL(WD.A30_PAY_SP_TAX_AMT, 0), 13, '*'), -13, 1), '*', '') AS SUM_NUM13
           , HRM_COMMON_DATE_G.DATE_YYYYMMDD_F(TO_DATE(TO_CHAR(WD.SUBMIT_DATE, 'YYYYMM') || '10', 'YYYY-MM-DD')) AS PAYMENT_DUE_DATE
        FROM HRW_WITHHOLDING_DOC WD
          , ( SELECT CM.CORP_NAME       -- 법인.
                  , CM.PRESIDENT_NAME   -- 대표자.
                  , CM.LEGAL_NUMBER
                  , ( SELECT OU.VAT_NUMBER
                        FROM HRM_OPERATING_UNIT OU
                      WHERE OU.CORP_ID        = CM.CORP_ID
                        AND (OU.DEFAULT_FLAG  = 'Y'
                        OR ROWNUM             <= 1)) AS VAT_NUMBER       -- 사업자번호.
                  , CM.ADDR1 || ' ' || CM.ADDR2 AS ADDRESS
                  , ( SELECT OU.TAX_OFFICE_NAME
                        FROM HRM_OPERATING_UNIT OU
                      WHERE OU.CORP_ID        = CM.CORP_ID
                        AND (OU.DEFAULT_FLAG  = 'Y'
                        OR ROWNUM             <= 1)) AS TAX_OFFICE_NAME  -- 관할세무서명.
                  , CM.TEL_NUMBER       -- 전화번호.
                  , CM.FAX_NUMBER
                  , CM.EMAIL
                  , CM.CORP_ID
              FROM HRM_CORP_MASTER CM
              WHERE CM.SOB_ID             = P_SOB_ID
                AND CM.ORG_ID             = P_ORG_ID
            ) HCM
      WHERE WD.CORP_ID                  = HCM.CORP_ID
        AND WD.WITHHOLDING_DOC_ID       = P_WITHHOLDING_DOC_ID
      ;
  END PRINT_WITHHOLDING_DOC_3;

-- 소득세납부서-퇴직소득.
  PROCEDURE PRINT_WITHHOLDING_DOC_4
            ( P_CURSOR2               OUT TYPES.TCURSOR2
            , P_WITHHOLDING_DOC_ID    IN NUMBER
            , P_SOB_ID                IN HRW_WITHHOLDING_DOC.SOB_ID%TYPE
            , P_ORG_ID                IN HRW_WITHHOLDING_DOC.ORG_ID%TYPE
            )
  AS
  BEGIN
    OPEN P_CURSOR2 FOR
      --사업소득.
      SELECT '0126' AS CLASSIFY_TYPE
           , '122' AS CITY_CODE
           , TO_CHAR(WD.SUBMIT_DATE, 'YYMM') AS SUBMIT_YYMM
           , '4' AS SUBMIT_TYPE
           , '21' AS TAX_TYPE
           , HCM.TAX_OFFICE_NAME
           , '110233' AS TAX_ACCOUNT_NUM
           , HCM.CORP_NAME
           , HCM.VAT_NUMBER
           , TO_CHAR(WD.SUBMIT_DATE, 'YYYY') AS FISCAL_YEAR
           , HCM.ADDRESS
           , HCM.TEL_NUMBER
           , SUBSTR(WD.STD_YYYYMM, 1, 4) AS STD_YEAR
           , SUBSTR(WD.STD_YYYYMM, 6, 2) AS STD_MONTH
           , REPLACE(SUBSTR(LPAD(NVL(WD.A20_PAY_INCOME_TAX_AMT, 0), 13, '*'), -1, 1), '*', '') AS INCOME_NUM1
           , REPLACE(SUBSTR(LPAD(NVL(WD.A20_PAY_INCOME_TAX_AMT, 0), 13, '*'), -2, 1), '*', '') AS INCOME_NUM2
           , REPLACE(SUBSTR(LPAD(NVL(WD.A20_PAY_INCOME_TAX_AMT, 0), 13, '*'), -3, 1), '*', '') AS INCOME_NUM3
           , REPLACE(SUBSTR(LPAD(NVL(WD.A20_PAY_INCOME_TAX_AMT, 0), 13, '*'), -4, 1), '*', '') AS INCOME_NUM4
           , REPLACE(SUBSTR(LPAD(NVL(WD.A20_PAY_INCOME_TAX_AMT, 0), 13, '*'), -5, 1), '*', '') AS INCOME_NUM5
           , REPLACE(SUBSTR(LPAD(NVL(WD.A20_PAY_INCOME_TAX_AMT, 0), 13, '*'), -6, 1), '*', '') AS INCOME_NUM6
           , REPLACE(SUBSTR(LPAD(NVL(WD.A20_PAY_INCOME_TAX_AMT, 0), 13, '*'), -7, 1), '*', '') AS INCOME_NUM7
           , REPLACE(SUBSTR(LPAD(NVL(WD.A20_PAY_INCOME_TAX_AMT, 0), 13, '*'), -8, 1), '*', '') AS INCOME_NUM8
           , REPLACE(SUBSTR(LPAD(NVL(WD.A20_PAY_INCOME_TAX_AMT, 0), 13, '*'), -9, 1), '*', '') AS INCOME_NUM9
           , REPLACE(SUBSTR(LPAD(NVL(WD.A20_PAY_INCOME_TAX_AMT, 0), 13, '*'), -10, 1), '*', '') AS INCOME_NUM10
           , REPLACE(SUBSTR(LPAD(NVL(WD.A20_PAY_INCOME_TAX_AMT, 0), 13, '*'), -11, 1), '*', '') AS INCOME_NUM11
           , REPLACE(SUBSTR(LPAD(NVL(WD.A20_PAY_INCOME_TAX_AMT, 0), 13, '*'), -12, 1), '*', '') AS INCOME_NUM12
           , REPLACE(SUBSTR(LPAD(NVL(WD.A20_PAY_INCOME_TAX_AMT, 0), 13, '*'), -13, 1), '*', '') AS INCOME_NUM13
           , TO_CHAR(NULL) AS SP_NUM1
           , TO_CHAR(NULL) AS SP_NUM2
           , TO_CHAR(NULL) AS SP_NUM3
           , TO_CHAR(NULL) AS SP_NUM4
           , TO_CHAR(NULL) AS SP_NUM5
           , TO_CHAR(NULL) AS SP_NUM6
           , TO_CHAR(NULL) AS SP_NUM7
           , TO_CHAR(NULL) AS SP_NUM8
           , TO_CHAR(NULL) AS SP_NUM9
           , TO_CHAR(NULL) AS SP_NUM10
           , TO_CHAR(NULL) AS SP_NUM11
           , TO_CHAR(NULL) AS SP_NUM12
           , TO_CHAR(NULL) AS SP_NUM13
           , REPLACE(SUBSTR(LPAD(NVL(WD.A20_PAY_INCOME_TAX_AMT, 0), 13, '*'), -1, 1), '*', '') AS SUM_NUM1
           , REPLACE(SUBSTR(LPAD(NVL(WD.A20_PAY_INCOME_TAX_AMT, 0), 13, '*'), -2, 1), '*', '') AS SUM_NUM2
           , REPLACE(SUBSTR(LPAD(NVL(WD.A20_PAY_INCOME_TAX_AMT, 0), 13, '*'), -3, 1), '*', '') AS SUM_NUM3
           , REPLACE(SUBSTR(LPAD(NVL(WD.A20_PAY_INCOME_TAX_AMT, 0), 13, '*'), -4, 1), '*', '') AS SUM_NUM4
           , REPLACE(SUBSTR(LPAD(NVL(WD.A20_PAY_INCOME_TAX_AMT, 0), 13, '*'), -5, 1), '*', '') AS SUM_NUM5
           , REPLACE(SUBSTR(LPAD(NVL(WD.A20_PAY_INCOME_TAX_AMT, 0), 13, '*'), -6, 1), '*', '') AS SUM_NUM6
           , REPLACE(SUBSTR(LPAD(NVL(WD.A20_PAY_INCOME_TAX_AMT, 0), 13, '*'), -7, 1), '*', '') AS SUM_NUM7
           , REPLACE(SUBSTR(LPAD(NVL(WD.A20_PAY_INCOME_TAX_AMT, 0), 13, '*'), -8, 1), '*', '') AS SUM_NUM8
           , REPLACE(SUBSTR(LPAD(NVL(WD.A20_PAY_INCOME_TAX_AMT, 0), 13, '*'), -9, 1), '*', '') AS SUM_NUM9
           , REPLACE(SUBSTR(LPAD(NVL(WD.A20_PAY_INCOME_TAX_AMT, 0), 13, '*'), -10, 1), '*', '') AS SUM_NUM10
           , REPLACE(SUBSTR(LPAD(NVL(WD.A20_PAY_INCOME_TAX_AMT, 0), 13, '*'), -11, 1), '*', '') AS SUM_NUM11
           , REPLACE(SUBSTR(LPAD(NVL(WD.A20_PAY_INCOME_TAX_AMT, 0), 13, '*'), -12, 1), '*', '') AS SUM_NUM12
           , REPLACE(SUBSTR(LPAD(NVL(WD.A20_PAY_INCOME_TAX_AMT, 0), 13, '*'), -13, 1), '*', '') AS SUM_NUM13
           , HRM_COMMON_DATE_G.DATE_YYYYMMDD_F(TO_DATE(TO_CHAR(WD.SUBMIT_DATE, 'YYYYMM') || '10', 'YYYY-MM-DD')) AS PAYMENT_DUE_DATE
        FROM HRW_WITHHOLDING_DOC WD
          , ( SELECT CM.CORP_NAME       -- 법인.
                  , CM.PRESIDENT_NAME   -- 대표자.
                  , CM.LEGAL_NUMBER
                  , ( SELECT OU.VAT_NUMBER
                        FROM HRM_OPERATING_UNIT OU
                      WHERE OU.CORP_ID        = CM.CORP_ID
                        AND (OU.DEFAULT_FLAG  = 'Y'
                        OR ROWNUM             <= 1)) AS VAT_NUMBER       -- 사업자번호.
                  , CM.ADDR1 || ' ' || CM.ADDR2 AS ADDRESS
                  , ( SELECT OU.TAX_OFFICE_NAME
                        FROM HRM_OPERATING_UNIT OU
                      WHERE OU.CORP_ID        = CM.CORP_ID
                        AND (OU.DEFAULT_FLAG  = 'Y'
                        OR ROWNUM             <= 1)) AS TAX_OFFICE_NAME  -- 관할세무서명.
                  , CM.TEL_NUMBER       -- 전화번호.
                  , CM.FAX_NUMBER
                  , CM.EMAIL
                  , CM.CORP_ID
              FROM HRM_CORP_MASTER CM
              WHERE CM.SOB_ID             = P_SOB_ID
                AND CM.ORG_ID             = P_ORG_ID
            ) HCM
      WHERE WD.CORP_ID                  = HCM.CORP_ID
        AND WD.WITHHOLDING_DOC_ID       = P_WITHHOLDING_DOC_ID
      ;
  END PRINT_WITHHOLDING_DOC_4;

