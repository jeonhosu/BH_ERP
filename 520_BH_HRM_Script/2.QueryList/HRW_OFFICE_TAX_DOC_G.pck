/******************************************************************************/
-- 지방소득세(종업원할 사업소세) 리스트.
  PROCEDURE SELECT_OFFICE_TAX_LIST
            ( P_CURSOR            OUT TYPES.TCURSOR
            , P_CORP_ID           IN NUMBER
            , P_SUBMIT_YEAR       IN VARCHAR2
            , P_SOB_ID            IN NUMBER
            , P_ORG_ID            IN NUMBER
            )
  AS
  BEGIN
    OPEN P_CURSOR FOR
      SELECT OD.OFFICE_TAX_NO
          , OD.OFFICE_TAX_TYPE
          , HRM_COMMON_G.CODE_NAME_F('WITHHOLDING_TYPE', OD.OFFICE_TAX_TYPE, OD.SOB_ID, OD.ORG_ID) AS OFFICE_TAX_DESC
          , OD.SUBMIT_DATE
          , OD.STD_YYYYMM
          , OD.PAY_YYYYMM
          , OD.TOTAL_TAX_AMT
          , OD.TAX_OFFICER
          , OD.CLOSED_YN
          , OD.OFFICE_TAX_ID
        FROM HRW_OFFICE_TAX_DOC OD
      WHERE OD.CORP_ID                  = P_CORP_ID
        AND OD.SOB_ID                   = P_SOB_ID
        AND OD.ORG_ID                   = P_ORG_ID
        AND TO_CHAR(OD.SUBMIT_DATE, 'YYYY') = P_SUBMIT_YEAR
      ORDER BY OD.OFFICE_TAX_NO DESC
      ;
  END SELECT_OFFICE_TAX_LIST;

