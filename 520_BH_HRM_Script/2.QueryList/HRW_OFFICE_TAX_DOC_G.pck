-- 원천징수이행상황신고서 인쇄.
  PROCEDURE PRINT_WITHHOLDING_DOC
            ( P_CURSOR2               OUT TYPES.TCURSOR2
            , P_WITHHOLDING_DOC_ID    IN NUMBER
            , P_SOB_ID                IN HRW_WITHHOLDING_DOC.SOB_ID%TYPE
            , P_ORG_ID                IN HRW_WITHHOLDING_DOC.ORG_ID%TYPE
            )
  AS
  BEGIN
    OPEN P_CURSOR2 FOR
      SELECT HCM.CORP_NAME
          , HCM.PRESIDENT_NAME
          , HCM.VAT_NUMBER
          , HCM.ADDRESS
          , HCM.TEL_NUMBER
          , HCM.EMAIL
          , HCM.WITHHOLDING_AGENT
          , HRM_COMMON_G.CODE_NAME_F('WITHHOLDING_TYPE', WD.WITHHOLDING_TYPE, WD.SOB_ID, WD.ORG_ID) AS WITHHOLDING_TYPE_DESC
          , SUBSTR(WD.STD_YYYYMM, 1, 4) || '년 ' || SUBSTR(WD.STD_YYYYMM, 6, 2) || '월' AS STD_YYYYMM
          , SUBSTR(WD.PAY_YYYYMM, 1, 4) || '년 ' || SUBSTR(WD.PAY_YYYYMM, 6, 2) || '월' AS PAY_YYYYMM
          , WD.INCOME_DISPOSED_YN
          , HRM_COMMON_DATE_G.DATE_YYYYMMDD_F(WD.SUBMIT_DATE) AS SUBMIT_DATE
          , WD.MONTHLY_YN
          , WD.HALF_YEARLY_YN
          , WD.MODIFY_YN
          , WD.YEAR_END_YN
          , WD.REFUND_REQUEST_YN
          , DECODE(WD.ALL_PAYMENT_YN, 'Y', '여', '부') AS ALL_PAYMENT_YN
          , DECODE(WD.BUSINESS_UNIT_TAX_YN, 'Y', '여', '부') AS BUSINESS_UNIT_TAX_YN
          , '북인천' AS TAX_OFFICE
          , WD.A01_PERSON_CNT
          , WD.A01_PAYMENT_AMT
          , WD.A01_INCOME_TAX_AMT
          , WD.A01_SP_TAX_AMT
          , WD.A01_ADD_TAX_AMT
          , WD.A02_PERSON_CNT
          , WD.A02_PAYMENT_AMT
          , WD.A02_INCOME_TAX_AMT
          , WD.A02_SP_TAX_AMT
          , WD.A02_ADD_TAX_AMT
          , WD.A03_PERSON_CNT
          , WD.A03_PAYMENT_AMT
          , WD.A03_INCOME_TAX_AMT
          , WD.A03_ADD_TAX_AMT
          , WD.A04_PERSON_CNT
          , WD.A04_PAYMENT_AMT
          , WD.A04_INCOME_TAX_AMT
          , WD.A04_SP_TAX_AMT
