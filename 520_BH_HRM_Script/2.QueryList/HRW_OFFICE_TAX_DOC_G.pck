-- 원천징수이행상황신고서 삭제.
  PROCEDURE DELETE_WITHHOLDING_DOC
            ( W_WITHHOLDING_DOC_ID         IN HRW_WITHHOLDING_DOC.WITHHOLDING_DOC_ID%TYPE
            , P_SOB_ID                     IN HRW_WITHHOLDING_DOC.SOB_ID%TYPE
            , P_ORG_ID                     IN HRW_WITHHOLDING_DOC.ORG_ID%TYPE
            )
  AS
  BEGIN
    -- 마감 여부 체크.
    IF HRW_WITHHOLDING_SET_G.CLOSED_WITHHOLDING_YN(W_WITHHOLDING_DOC_ID) = 'Y' THEN
      -- 이미 마감처리됨.
      RAISE_APPLICATION_ERROR(-20001, EAPP_MESSAGE_G.RETURN_MSG_F('FCM_10052'));
      RETURN;
    END IF;

    DELETE FROM HRW_WITHHOLDING_DOC
    WHERE WITHHOLDING_DOC_ID        = W_WITHHOLDING_DOC_ID;
  END DELETE_WITHHOLDING_DOC;

