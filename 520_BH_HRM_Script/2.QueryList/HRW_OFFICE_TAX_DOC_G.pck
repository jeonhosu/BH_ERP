    BEGIN
      SELECT NVL(MAX(SUBSTR(WD.WITHHOLDING_NO, 6, 4)), 0) + 1 AS NEXT_SEQ
        INTO V_SEQ
        FROM HRW_WITHHOLDING_DOC WD
      WHERE WD.CORP_ID            = P_CORP_ID
        AND WD.SOB_ID             = P_SOB_ID
        AND WD.ORG_ID             = P_ORG_ID
        AND TO_CHAR(WD.SUBMIT_DATE, 'YYYY') = V_SUBMIT_YEAR
